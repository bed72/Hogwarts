<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape android:shape="rectangle" >
            <solid android:color="@color/transparent" />

            <size
                android:width="@dimen/check_box_select_size"
                android:height="@dimen/check_box_select_size" />

            <corners
                android:radius="@dimen/check_box_select_corner_radius" />

            <stroke
                android:width="@dimen/check_box_select_stroke_radius"
                android:color="@color/dark_secondary" />
        </shape>
    </item>
</layer-list>